{
  "expo": {
    "name": "NurseLinkAI",
    "slug": "nurselink-ai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nurselinkai.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "NurseLinkAI utilise votre localisation pour vous proposer des missions proches de chez vous.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "NurseLinkAI utilise votre localisation pour vous proposer des missions proches et optimiser vos d√©placements.",
        "UIBackgroundModes": [
          "location",
          "remote-notification"
        ]
      }
    },
    "android": {
      "backgroundColor": "#2563eb",
      "package": "com.nurselinkai.mobile",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "NurseLinkAI utilise votre localisation pour vous proposer des missions proches de chez vous."
        }
      ],
      [
        "expo-notifications"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
