{
  "name": "nurselink-ai-mobile",
  "version": "1.0.0",
  "description": "NurseLink AI - Application mobile pour infirmiers et établissements de santé",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "EXPO_NO_DAEMON=1 npx expo start --offline",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "dev": "EXPO_NO_DAEMON=1 npx expo start --dev-client --offline",
    "clear": "rm -rf .expo && rm -rf node_modules/.cache && rm -rf .metro-cache"
  },
  "dependencies": {
    "@expo/metro-config": "^0.20.15",
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
