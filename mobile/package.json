    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "expo": "~53.0.0",
    "expo-constants": "~17.1.6",
    "expo-device": "~7.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-location": "~18.1.5",
    "expo-notifications": "~0.31.3",
    "expo-secure-store": "~14.2.3",
